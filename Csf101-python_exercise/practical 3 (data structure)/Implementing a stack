class Stack:
    def __init__(Self):
        Self.items = []
    def is_empty(Self):
        return len(Self.items) == 0
    def push(self, item):
        self.items.append(item)
    def pop(self):
        if not self.is_empty():
            return self.items.pop ()
    
        else:
            raise IndexError("Stack is empty")
    def peek (self):
        if not self.is_empty():
            return self.items[-1]
        else:
            raise IndexError ("Stack is empty")
    def size(self):
            return len(self.items)
    
stack = Stack()
stack.push(1)
stack.push(2)
stack.push(3)
print(stack.pop())  
print(stack.peek())  
print(stack.size()) 
    